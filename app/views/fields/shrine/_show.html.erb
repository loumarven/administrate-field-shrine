<% if field.multiple? -%>
  <%= render partial: 'fields/shrine/show_multiple', locals: { field: field } %>
<% else -%>
  <% if field.url_only? -%>
    <%= link_to field.url, field.url if field.url.present? %>
  <% else -%>
    <%= image_tag field.url if field.url.present? %>
  <% end -%>
<% end -%>
